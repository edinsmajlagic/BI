  <DataSets>
    <DataSet Name="Radnici">
      <Query>
        <DataSourceName>PredskolskaUstanova</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PosaoID">
            <Value>=Parameters!PosaoID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StarostOd">
            <Value>=Parameters!StarostOd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StarostDo">
            <Value>=Parameters!StarostDo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StazOd">
            <Value>=Parameters!StazOd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StazDo">
            <Value>=Parameters!StazDo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT R.Ime+' '+R.Prezime AS Radnik,
            TP.TipPosla AS [Tip posla],
		YEAR(GETDATE())-YEAR(R.DatumRodjenja) AS Starost,
		YEAR(GETDATE())-YEAR(R.DatumZaposljavanja) AS Staž
FROM Radnik AS R
     INNER JOIN TipPosla AS TP ON R.PosaoID=TP.PosaoID
WHERE TP.PosaoID IN(@PosaoID) AND
      (YEAR(GETDATE())-YEAR(R.DatumRodjenja)&gt;= @StarostOd OR @StarostOd IS NULL) AND
	  (YEAR(GETDATE())-YEAR(R.DatumRodjenja)&lt;= @StarostDo OR @StarostDo IS NULL) AND
	  (YEAR(GETDATE())-YEAR(R.DatumZaposljavanja)&gt;= @StazOd OR @StazOd IS NULL) AND
	  (YEAR(GETDATE())-YEAR(R.DatumZaposljavanja)&lt;= @StazDo OR @StazDo IS NULL)</CommandText>
      </Query>
