  <DataSets>
    <DataSet Name="Djeca">
      <Query>
        <DataSourceName>PredskolskaUstanova</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@GrupaID">
            <Value>=Parameters!GrupaID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SocijalnoStanjeID">
            <Value>=Parameters!SocijalnoStanjeID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SpecijalnaPotrebaID">
            <Value>=Parameters!SpecijalnaPotrebaID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PlanProgramID">
            <Value>=Parameters!PlanProgramID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT D.DijeteID,D.Ime+'('+R.Ime+')'+D.Prezime AS Dijete,
       CONVERT(NVARCHAR,D.DatumRodjenja,104)AS [Datum rođenja],
	   G.Naziv AS [Mjesto rođenja],
	   Gr.StarosnaDob AS [Starosna dob]
FROM Dijete AS D
     INNER JOIN Roditelj AS R ON D.RoditeljID=R.RoditeljID
	 INNER JOIN Grad AS G ON D.GradID=G.GradID
	 INNER JOIN Grupa AS Gr ON D.GrupaID=Gr.GrupaID
	 INNER JOIN SocijalnoStanje AS SS ON D.SocijalnoStanjeID=SS.SocijalnoStanjeID
	 INNER JOIN DijeteSpecijalnaPotreba AS DSP ON D.DijeteID=DSP.DijeteID
	 INNER JOIN SpecijalnaPotreba AS SP ON DSP.SpecijalnaPotrebaID=SP.SpecijalnaPotrebaID
	 INNER JOIN PlanProgram AS PP ON D.PlanProgramID=PP.PlanProgramID
WHERE Gr.GrupaID IN(@GrupaID)
      AND SS.SocijalnoStanjeID IN(@SocijalnoStanjeID)
      AND SP.SpecijalnaPotrebaID IN(@SpecijalnaPotrebaID)
      AND PP.PlanProgramID IN(@PlanProgramID)
ORDER BY D.DijeteID</CommandText>
      </Query>
      <Fields>
        <Field Name="DijeteID">
          <DataField>DijeteID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Dijete">
          <DataField>Dijete</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Datum_rođenja">
          <DataField>Datum rođenja</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Mjesto_rođenja">
          <DataField>Mjesto rođenja</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Starosna_dob">
          <DataField>Starosna dob</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
